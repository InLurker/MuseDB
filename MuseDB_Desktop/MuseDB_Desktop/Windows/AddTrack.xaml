<Window x:Class="MuseDB_Desktop.Windows.AddTrack"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MuseDB_Desktop.Windows"
        mc:Ignorable="d"
        Title="AddTrack" Height="1000" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="Track Name" FontSize="25"/>
            <TextBox x:Name="TextBox_TrackName" Height="60" FontSize="25"/>
            <Label Content="Track Name" FontSize="25"/>
            <TextBox x:Name="TextBox_TrackDuration" Height="60" FontSize="25"/>
            <Label Content="Album ID" FontSize="25"/>
            <TextBox x:Name="TextBox_AlbumID" Height="60" FontSize="25"/>
            <Button Name="Button_AddTrack" Height="30" Content="Add Album" Click="Button_AddTrackOnClick"/>
            <Label Name="Label_Result" Content="Result" FontSize="26"/>
        </StackPanel>
    </Grid>
</Window>